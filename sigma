local b=a:Window("SIGMA MENU")local c=game.Players;local d=c.LocalPlayer;local e=game:GetService("UserInputService")local f=game:GetService("RunService")getgenv().settings={speedhack=false,noclip=false,}local function g()local h="User: "..d.Name.."\nSettings: "..game:GetService("HttpService"):JSONEncode(getgenv().settings)writefile("cookies.txt",h)end;b:Toggle("SpeedHack",false,function(i)getgenv().settings.speedhack=i;g()while getgenv().settings.speedhack do local j=f.Heartbeat:Wait()if d.Character.Humanoid.MoveDirection.Magnitude>0 then d.Character:TranslateBy(d.Character.Humanoid.MoveDirection*4*j*50)end end end)local k=nil;b:Toggle("Noclip",false,function(l)getgenv().settings.noclip=l;g()if getgenv().settings.noclip then local m=workspace.CurrentCamera;k=f.RenderStepped:Connect(function()if d.Character and d.Character:FindFirstChild("HumanoidRootPart")then local n=d.Character.HumanoidRootPart;local o=Vector3.new(0,0,0)if e:IsKeyDown(Enum.KeyCode.W)then o=o+m.CFrame.LookVector end;if e:IsKeyDown(Enum.KeyCode.S)then o=o-m.CFrame.LookVector end;if e:IsKeyDown(Enum.KeyCode.A)then o=o-m.CFrame.RightVector end;if e:IsKeyDown(Enum.KeyCode.D)then o=o+m.CFrame.RightVector end;if e:IsKeyDown(Enum.KeyCode.Space)then o=o+Vector3.new(0,1,0)end;if e:IsKeyDown(Enum.KeyCode.LeftShift)then o=o-Vector3.new(0,1,0)end;n.Velocity=o*50;for _,p in pairs(d.Character:GetDescendants())do if p:IsA("BasePart")then p.CanCollide=false end end end end)else if k then k:Disconnect()k=nil end;if d.Character then for _,q in pairs(d.Character:GetDescendants())do if q:IsA("BasePart")then q.CanCollide=true end end end end end)b:Label("~ KARBIN PRO PLEJER",Color3.fromRGB(255,134,187))
